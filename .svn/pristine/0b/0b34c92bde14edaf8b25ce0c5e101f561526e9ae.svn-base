    <script>
        $(function () {
            $('.Date').datepicker({
                changeMonth: true,
                changeYear: true,
                dateFormat: "yy-mm-dd"
            });
            $('.nepali-calendar').nepaliDatePicker({
                npdMonth: true,
                npdYear: true,
            });
            var button = $('#loginButton');
            var box = $('#loginBox');
            var form = $('#loginForm');
            button.removeAttr('href');
            button.mouseup(function (login) {
                box.toggle();
                button.toggleClass('active');
            });
            form.mouseup(function () {
                return false;
            });
        });
    </script>
